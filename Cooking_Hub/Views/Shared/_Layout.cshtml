				form.method = 'post';

				// Create and append the anti-forgery token input element
				var tokenInput = document.createElement('input');
				tokenInput.type = 'hidden';
				tokenInput.name = '__RequestVerificationToken';
				tokenInput.value = getAntiForgeryToken(); // Call the function to retrieve the anti-forgery token
				form.appendChild(tokenInput);

				document.body.appendChild(form);
				form.submit();
			}

			// Function to retrieve the anti-forgery token from the page's hidden input field
			function getAntiForgeryToken() {
				var tokenInput = document.querySelector('input[name="__RequestVerificationToken"]');
				if (tokenInput) {
					return tokenInput.value;
